  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftcarrot/react-input-switch.git"
  },
  "author": "Wang Zuo <wangzuo@swiftcarrot.com> (https://swiftcarrot.com)",
  "license": "MIT",
  "keywords": [
    "react-component",
    "switch",
    "input",
    "checkbox"
  ],
  "bugs": {
    "url": "https://github.com/swiftcarrot/react-input-switch/issues"
  },
  "homepage": "https://swiftcarrot.dev/react-input-switch",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@emotion/core": "^10.0.14"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "babel-preset-swiftcarrot": "^1.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "jest": {
